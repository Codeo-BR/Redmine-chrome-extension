{
  "name": "redmine-chrome-extension",
  "version": "0.9.0",
  "manifest_version": 2,
  "icons": {
      "128": "icon.png"
  },

  "description": "Redmine Chrome extension.",
  "permissions" : [
    "cookies",
    "<all_urls>",
    "storage"
  ],  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Redmine Chrome Extension"
  }
}